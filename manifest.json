{
    "manifest_version": 2, 
    "name": "Aerorequestor",
    "description": "Une application permettant d'obtenir de nombreuse données intéressante concernant l'aéronautique",
    "version": "0.8",
    "permissions": [ 
     "http://*/*",
     "https://*/*"
    ],
    
    "browser_action": {   
     "default_icon" : "image/airplane_icon.png", 
     "default_popup": "app.html" 
    },
    "icons": { 
     "128" : "image/airplane_icon.png" 
    }
   }